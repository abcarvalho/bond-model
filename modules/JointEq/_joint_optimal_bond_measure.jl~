

function find_optimal_bond_measure(svm;
                                   jks=JointKStruct(fill(NaN, 10)...),
                                   mu_s::Float64=NaN,
                                   mu_b_min::Float64=.5,
                                   mu_b_max::Float64=15.;
                                   mu_bN::Int64=15;
                                   m::Float64=NaN,
                                   c::Float64=NaN,
                                   p::Float64=NaN,
                                   lb::Float64=.75,
                                   ub::Float64=1.25,
                                   vbN::Int64=15,
                                   N::Int64=10^5
                                   
                                   )

    # Set Capital Structure #########################
    jks = joint_eq_set_k_struct!(jf; jks=jks,
                                 mu_s=mu_s,
                                 mu_b=mu_b_min,
                                 m=m, c=c, p=p,
                                 fi_sf_vb=fi_sf_vb,
                                 sf_vb=sf_vb,
                                 fi_rf_vb=fi_rf_vb,
                                 rf_vb=rf_vb)
    # ###############################################

    # Form mu_b grid
    mu_b = range(mu_b_min, stop=mu_b_max; length=mu_bN)

    vbs = find_full_info_vb(svm, jks;
                           lb::Float64=.75, ub::Float64=1.25,
                           vbN::Int64=15, N::Int64=10^5)
    res = [eq_fd(svm;]
                                   

end
